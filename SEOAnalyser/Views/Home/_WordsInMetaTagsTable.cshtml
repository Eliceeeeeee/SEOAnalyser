
@using Kendo.Mvc.UI
@model IEnumerable<SEOAnalyser.Web.Models.WordsInMetaTagsViewModel>
@if (Model != null)
{
	@(Html.Kendo().Grid(Model)
			.Name("WordsInMetaTagsTable")
			.Columns(columns =>
			{
				columns.Bound(x => x.WordInMetaTag)
					.Sortable(false)
					.Title("Word In Meta Tag");
				columns.Bound(x => x.NoOfWordInMetaTags)
					.Sortable(false)
					.Title("No Of Word In Meta Tag");
			})
			.DataSource(datasource => datasource.Server())
	)
}